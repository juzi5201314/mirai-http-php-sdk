{
  "name": "soeur/mirai-http-php-sdk",
  "authors": [
    {
      "name": "soeur",
      "email": "juzi201314@gmail.com"
    }
  ],
  "description": "一个基于mirai http api的qq机器人框架",
  "autoload": {
    "psr-4": {
      "MiraiSdk\\": "src",
      "MiraiSdk\\tests\\": "tests"
    },
    "files": [
      "src/utils.php"
    ]
  },
  "require": {
    "amphp/log": "v1.1.0",
    "phpunit/phpunit": "9.1.5",
    "amphp/http-client": "v4.3.1",
    "ext-json": "*",
    "php": ">=7.4",
    "guzzlehttp/guzzle": "6.5.4"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  ],
  "require-dev": {
    "amphp/phpunit-util": "v1.4.0",
    "symfony/dotenv": "v5.1.0"
  }
}
